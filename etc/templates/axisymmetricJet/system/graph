FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      ;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

graph
{
    type            sets;
    libs ("libsampling.so");
    writeControl   writeTime;

    interpolationScheme cellPointFace;

    setFormat       raw;

    sets
    (
        centreline
        {
            type    lineUniform;
            axis    x;
            start   ( 0  1e-6 0);
            end     (25  1e-6 0);
            nPoints 250;
        }
    );

    fields      ( U );
}

