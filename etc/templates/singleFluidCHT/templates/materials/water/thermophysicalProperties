FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      thermophysicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type            heRhoThermo;
    mixture         pureMixture;
    transport       const;
    thermo          hConst;
    equationOfState perfectFluid;
    specie          specie;
    energy          sensibleEnthalpy;
}

mixture
{
    specie
    {
        nMoles      1;
        molWeight   18.0;       // [g/mol]
    }
    equationOfState
    {
        R           3000;      // [J/kg/K]
        rho0        1027;      // [kg/m^3]
    }
    thermodynamics
    {
        Cp          4181;      // [J/kg/K] at T = 293 K
        Hf          0;
    }
    transport
    {
        mu          1.0e-03;    // [kg/m/s]
        Pr          7;
    }
}


