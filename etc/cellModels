// Equal to sammTrim8
hexagonalPrism
{
    index 18;
    numberOfPoints 12;

    faces
    8
    (
        6(0 5 4 3 2 1)    // bottom
        6(6 7 8 9 10 11)  // top
        4(0 1 7 6)        // sides
        4(1 2 8 7)
        4(2 3 9 8)
        4(3 4 10 9)
        4(4 5 11 10)
        4(5 0 6 11)
    );

    edges
    18
    (
        (0 1)            // bottom
        (1 2)
        (2 3)
        (3 4)
        (4 5)
        (5 0)
        (6 7)            // top
        (7 8)
        (8 9)
        (9 10)
        (10 11)
        (11 6)
        (0 6)            // sides
