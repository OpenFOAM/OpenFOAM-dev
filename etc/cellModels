// split-hex. Used for embedded refinement
splitHex
{
    index 8;
    numberOfPoints 10;

    faces
    7
    (
        4(9 3 0 8)   // x-min1
        4(7 9 8 4)   // x-min2
        4(5 1 2 6)   // x-max
        4(4 0 1 5)   // y-min
        4(2 3 7 6)   // y-max
