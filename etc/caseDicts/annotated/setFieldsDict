FoamFile
{
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultValues
{
    alpha1  0;
    U       (0 0 0);
}

zones
{
    // Set cell values
    // (does zerogradient on boundaries)
    cells
    {
        type        box;
        zoneType    cell;

        box (0 0 -1) (0.1461 0.292 1);

        fieldValues
        (
            volScalarFieldValue alpha1 1
        );
    }

    // Set patch values (using ==)
    patchFaces
    {
        type        box;
        zoneType    face;

        box (0 0 -1) (0.1461 0.292 1);

        values
        {
            alpha1  1;
        }
    }
}

// ************************************************************************* //
