functions
{
    readFields1
    {
        type            readFields;
        libs ("libfieldFunctionObjects.so");
        enabled         true;
        writeControl   timeStep;
        writeInterval  1;

        fields
        (
            interpolateU
        );
    }

    faceObj2
    {
        type            faceSource;
        libs ("libfieldFunctionObjects.so");
        enabled         true;
        writeControl   timeStep;
        writeInterval  1;
        log             true;
        valueOutput     true;
        source          faceZone;
        sourceName      f0;
        operation       areaAverage;

        fields
        (
            interpolateU
        );
    }
}

