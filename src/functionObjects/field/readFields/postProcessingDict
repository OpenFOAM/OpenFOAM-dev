functions
{
    readFields1
    {
        type            readFields;
        libs            ("libfieldFunctionObjects.so");
        enabled         true;
        writeControl    timeStep;
        writeInterval   1;

        fields
        (
            interpolateU
        );
    }

    faceObj2
    {
        type            surfaceFieldValue;
        libs            ("libfieldFunctionObjects.so");
        enabled         true;
        writeControl    timeStep;
        writeInterval   1;
        log             true;
        writeFields     true;
        regionType      faceZone;
        name            f0;
        operation       areaAverage;

        fields
        (
            interpolateU
        );
    }
}

