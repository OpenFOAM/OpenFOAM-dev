FoamFile
{
    format      ascii;
    class       volVectorField;
    object      boundaryU;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    flow
    {
        type            fixedValue;
        value           uniform (1936 0 0);
    }

    "entrance.*"
    {
        type            symmetryPlane;
    }

    walls
    {
        type            noSlip;
    }
}

