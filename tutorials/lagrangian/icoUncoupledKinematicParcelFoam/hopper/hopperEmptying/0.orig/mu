FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      mu;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -1 0 0 0 0];

internalField   uniform 1.2e-05;

boundaryField
{
    walls
    {
        type            calculated;
        value           uniform 1.2e-05;
    }
    frontAndBack
    {
        type            calculated;
        value           uniform 1.2e-05;
    }
    inlet
    {
        type            calculated;
        value           uniform 1.2e-05;
    }
    outlet
    {
        type            calculated;
        value           uniform 1.2e-05;
    }
}


// ************************************************************************* //
