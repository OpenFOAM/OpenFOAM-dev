FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    walls
    {
        type            fixedFluxPressure;
        phi             phi.air;
        value           $internalField;
    }

    lowerInlet
    {
        type            fixedFluxPressure;
        phi             phi.air;
        value           $internalField;
    }

    upperInlet
    {
        type            fixedFluxPressure;
        phi             phi.air;
        value           $internalField;
    }

    outlet
    {
        type            fixedValue;
        phi             phi.air;
        value           uniform 0;
    }

    frontAndBack        
    {
        type            symmetry;
    }
}

// ************************************************************************* //
