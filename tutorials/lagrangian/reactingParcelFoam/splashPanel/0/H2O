FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      H2O;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    top
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
    region0_to_wallFilm_walls
    {
        type            zeroGradient;
    }
}


