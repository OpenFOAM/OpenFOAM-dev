FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultValues
{
    alpha.water 1;
    p_rgh       1e5;
    p           1e5;
    T           300;
}

zones
{
    bubble
    {
        type        sphere;
        zoneType    cell;

        centre (0.5 0.5 0.5);
        radius 0.1;

        values
        {
            alpha.water 0;
            p_rgh       1e6;
            p           1e6;
            T           578;
        }
    }

    air
    {
        type        box;
        zoneType    cell;

        box         (-10 1 -1) (10 10 1);

        values
        {
            alpha.water 0;
        }
    }
}

// ************************************************************************* //
