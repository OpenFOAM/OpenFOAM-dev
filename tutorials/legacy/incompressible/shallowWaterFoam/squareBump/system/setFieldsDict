FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultValues
{
    h0      0;
    h       0.01;
    hU      (0.001 0 0);
}

zones
{
    bump
    {
        type        box;

        box         (0.45 0.45 0) (0.55 0.55 0.1);

        values
        {
            h0      0.001;
            h       0.009;
            hU      (0.0009 0 0);
        }
    }
}

