FoamFile
{
    format      ascii;
    class       volScalarField;
    object      boundaryT;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 0;

boundaryField
{
    flow
    {
        type            fixedValue;
        value           uniform 300;
    }

    obstacle
    {
        type            fixedValue;
        value           uniform 550;
    }

    frontAndBack
    {
        type            empty;
    }
}

