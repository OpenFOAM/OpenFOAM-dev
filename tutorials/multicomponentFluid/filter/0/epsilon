FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "1";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.0449;

boundaryField
{
    walls
    {
        type            epsilonWallFunction;
        value           uniform 0.0449;
    }
    inlet
    {
        type            fixedValue;
        value           uniform 0.0449;
    }
    outlet
    {
        type            zeroGradient;
    }
    cycLeft_half0
    {
        type            cyclic;
    }
    cycRight_half0
    {
        type            cyclic;
    }
    frontAndBack
    {
        type            empty;
    }
    cycLeft_half1
    {
        type            cyclic;
    }
    cycRight_half1
    {
        type            cyclic;
    }
}

sources
{
    massSource
    {
        type            turbulentMixingLengthDissipationRate;
        mixingLength    0.01;
    }
}


