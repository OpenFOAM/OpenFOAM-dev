FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvConstraints;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

limitp
{
    type       limitPressure;

    minFactor  0.5;
    maxFactor  2;
}

source1
{
    type            fixedTemperatureConstraint;

    selectionMode   cellZone;
    cellZone        porosity;

    mode            uniform;
    temperature     350;
}


