FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      fvOptions;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

volumeFraction
{
    type            volumeFractionSource;
    phase           solid;
    phi             phi;
    rho             rho;
    U               U;
    fields          (rho U e);
}

solidEqulibriumEnergy
{
    type            solidEqulibriumEnergySource;
    phase           solid;
    field           e;
}

