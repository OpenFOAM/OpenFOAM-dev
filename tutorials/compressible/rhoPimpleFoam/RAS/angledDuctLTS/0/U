FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (47.8 0 0);

boundaryField
{
    walls
    {
        type            noSlip;
    }

    porosityWall
    {
        type            slip;
        value           uniform (0 0 0);
    }

    inlet
    {
        type            flowRateInletVelocity;
        massFlowRate    constant 0.1;
        value           uniform (0 0 0);
    }

    outlet
    {
        type            fluxCorrectedVelocity; // inletOutlet;
        value           uniform (0 0 0);
        inletValue      uniform (0 0 0);
    }
}

// ************************************************************************* //
