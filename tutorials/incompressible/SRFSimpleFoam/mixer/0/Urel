FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      Urel;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 -10);

boundaryField
{
    inlet
    {
        type            SRFVelocity;
        inletValue      uniform (0 0 -10);
        relative        no;
        value           uniform (0 0 0);
    }
    outlet
    {
        type            pressureInletOutletVelocity;
        value           $internalField;
    }
    innerWall
    {
        type            noSlip;
    }
    outerWall
    {
        type            SRFVelocity;
        inletValue      uniform (0 0 0);
        relative        no;
        value           uniform (0 0 0);
    }
    cyclic_half0
    {
        type            cyclic;
    }
    cyclic_half1
    {
        type            cyclic;
    }
}


// ************************************************************************* //
