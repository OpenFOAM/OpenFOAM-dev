FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 3.5;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 3.5;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 3.5;
    }
    innerWall
    {
        type            omegaWallFunction;
        value           uniform 3.5;
    }
    outerWall
    {
        type            omegaWallFunction;
        value           uniform 3.5;
    }
    cyclic_half0
    {
        type            cyclic;
        value           uniform 3.5;
    }
    cyclic_half1
    {
        type            cyclic;
        value           uniform 3.5;
    }
}


// ************************************************************************* //
