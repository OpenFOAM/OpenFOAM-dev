{
    type            MRFSource;
    active          true;
    selectionMode   cellZone;
    cellZone        rotor;

    MRFSourceCoeffs
    {
        active      true;
        origin      (0 0 0);
        axis        (0 0 1);
        omega       104.72;
    }
}


