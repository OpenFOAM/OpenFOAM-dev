Description
    Writes out files of streamlines with interpolated field data in VTK format.

\*---------------------------------------------------------------------------*/

direction       both;

seedSampleSet
{
    type            lineUniform;
    start           (-0.0205 0.001  0.00001);
    end             (-0.0205 0.0251 0.00001);
    nPoints         10;
    axis            x;
}

fields          (p k U);

#includeEtc "caseDicts/postProcessing/visualisation/streamlines.cfg"

