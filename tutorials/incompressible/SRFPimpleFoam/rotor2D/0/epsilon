FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 3.75e-4;

boundaryField
{
    rotor
    {
        type            epsilonWallFunction;
        U               Urel;
        value           uniform 3.75e-4;
    }

    freestream
    {
        type            inletOutlet;
        inletValue      uniform 3.75e-4;
    }

    front
    {
        type            empty;
    }

    back
    {
        type            empty;
    }
}


