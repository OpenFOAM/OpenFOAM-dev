FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      dynamicMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

mover
{
    type            motionSolver;

    libs            ("libfvMeshMovers.so" "libfvMotionSolvers.so");

    motionSolver    solidBody;

    cellZone        ball;

    solidBodyMotionFunction oscillatingRotatingMotion;

    origin        (0 0 0);
    amplitude     (0 0 45);
    omega         6.283185307179586;
}


