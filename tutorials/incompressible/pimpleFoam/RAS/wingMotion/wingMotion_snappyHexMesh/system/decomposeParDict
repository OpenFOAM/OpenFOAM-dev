FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      decomposeParDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

numberOfSubdomains 3;

method          simple;

simpleCoeffs
{
    n               (1 3 1);
}

hierarchicalCoeffs
{
    n               (3 2 1);
    order           xyz;
}

manualCoeffs
{
    dataFile        "cellDecomposition";
}

