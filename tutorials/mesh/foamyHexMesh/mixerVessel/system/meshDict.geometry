FoamFile
{
    version         2.0;
    format          ascii;
    class           dictionary;
    object          meshDict.geometry;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

vessel.stl
{
    name vessel;
    type triSurfaceMesh;

    regions
    {
        outlet {name outlet;}
        vessel {name vessel;}
    }
}


spargerInlet.stl
{
    name spargerInlet;
    type triSurfaceMesh;

    regions
    {
        oilInlet
        {
            name oilInlet;
        }

        sparger
        {
            name sparger;
        }
    }
}


