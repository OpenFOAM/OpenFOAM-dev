FoamFile
{
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    outer
    {
        type            uniformInletOutlet;
        uniformInletValue (0 0 0);
        value           uniform (0 0 0);
    }

    ground
    {
        type            noSlip;
    }

    blockedFaces
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    baffleWall
    {
        type            activePressureForceBaffleVelocity;
        value           uniform (0 0 0);
        cyclicPatch     baffleCyclic_half0;
        orientation     1;
        openingTime     0.01;
        maxOpenFractionDelta 0.1;
        openFraction    0;
        minThresholdValue 8000;
        forceBased      0;
    }

    baffleCyclic_half0
    {
        type            cyclic;
    }

    baffleCyclic_half1
    {
        type            cyclic;
    }
}


