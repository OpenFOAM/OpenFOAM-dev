pyrolysis
{
    active          false;

    pyrolysisModel  none;

    regionName      panelRegion;

    reactingOneDimCoeffs
    {
        radFluxName     Qr;

        minimumDelta    1e-8;

        moveMesh        false;
    }

    infoOutput      false;
}


