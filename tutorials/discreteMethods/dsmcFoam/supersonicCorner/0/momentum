FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      momentum;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -2 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    flow
    {
        type            zeroGradient;
    }

    "entrance.*"
    {
        type            symmetryPlane;
    }

    walls
    {
        type            calculated;
        value           uniform (0 0 0);
    }

}

