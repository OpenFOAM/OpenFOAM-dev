FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      boundaryU;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    outlet
    {
        type            fixedValue;
        value           uniform (1325 -352 823);
    }

    inlet
    {
        type            fixedValue;
        value           uniform (1325 -352 823);
    }

    sides
    {
        type            fixedValue;
        value           uniform (1325 -352 823);
    }
}

