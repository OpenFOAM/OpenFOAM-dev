FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    periodicX_half0
    {
        type            cyclic;
    }
    periodicY_half0
    {
        type            cyclic;
    }
    periodicZ_half0
    {
        type            cyclic;
    }
    periodicY_half1
    {
        type            cyclic;
    }
    periodicZ_half1
    {
        type            cyclic;
    }
    periodicX_half1
    {
        type            cyclic;
    }
}


