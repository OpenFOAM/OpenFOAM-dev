FoamFile
{
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    inlet
    {
        type fixedFluxPressure;
        value $internalField;
    }

    outlet
    {
        type fixedValue;
        value uniform 0;
    }

    walls
    {
        type fixedFluxPressure;
        value $internalField;
    }
}


