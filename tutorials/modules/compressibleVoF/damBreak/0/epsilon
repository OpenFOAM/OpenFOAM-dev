FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.1;

boundaryField
{
    leftWall
    {
        type            epsilonWallFunction;
        value           uniform 0.1;
    }

    rightWall
    {
        type            epsilonWallFunction;
        value           uniform 0.1;
    }

    lowerWall
    {
        type            epsilonWallFunction;
        value           uniform 0.1;
    }

    atmosphere
    {
        type            inletOutlet;
        inletValue      uniform 0.1;
        value           uniform 0.1;
    }

    defaultFaces
    {
        type            empty;
    }
}


// ************************************************************************* //
