FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    "(inlet|outlet)"
    {
        type            entrainmentPressure;
        p0              $internalField;
        value           $internalField;
    }

    wall
    {
        type            fixedFluxPressure;
    }

    nonCouple
    {
        type            fixedFluxPressure;
    }
}


// ************************************************************************* //
