FoamFile
{
    format      binary;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 1.4e-6;

boundaryField
{
    bottom
    {
        type            epsilonWallFunction;
        value           $internalField;
    }
    "(top|inlet|outlet)"
    {
        type            inletOutlet;
        value           $internalField;
        inletValue      $internalField;
    }
    "(front|back)"
    {
        type            symmetryPlane;
    }
}


// ************************************************************************* //
