FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0/film";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform  300;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    surface
    {
        type            coupledTemperature;
        value           $internalField;
    }

    wall
    {
        type            zeroGradient;
    }
}


