FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 1e-7;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    sides
    {
        type            nutkWallFunction;
        value           uniform 0;
    }

    film
    {
        type            nutkWallFunction;
        value           uniform 0;
    }
}

