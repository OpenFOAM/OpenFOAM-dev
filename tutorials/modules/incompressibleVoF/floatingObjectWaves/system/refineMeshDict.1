FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      refineMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

set box;

coordinateSystem global;

globalCoeffs
{
    e1   (1 0 0);
    e2   (0 1 0);
}

directions
(
    e1 e2 e3
);

useHexTopology  true;

geometricCut    false;

writeMesh       false;


