FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     foamMultiRun;

regionSolvers
{
    fluid           fluid;
    liner           solid;
    piston          solid;
    cylinderHead    solid;
    exhaustValve    solid;
    intakeValve     solid;
}

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         1440;

deltaT          0.05;

writeControl    adjustableRunTime;

writeInterval   5;

purgeWrite      0;

writeFormat     binary;

writePrecision  10;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  no;

maxCo           2;

maxDeltaT       1;

userTime
{
    type            engine;
    omega           60 [rpm];
}

