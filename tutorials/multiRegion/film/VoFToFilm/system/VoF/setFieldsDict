FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultValues
{
    alpha.liquid    0;
}

zones
{
    liquid
    {
        type        box;
        zoneType    cell;

        box         (0 0 -1) (1 0.08 1);

        values
        {
            alpha.liquid    1;
        }
    }
}

