#includeFunc populationBalanceSizeDistribution
(
    name=probabilityDensity.afterBend_1d,
    populationBalance=agglomerates,
    cellZone=
    {
        type        box;
        box         (-0.03 0.43 0.09) (0.03 0.48 0.11);
    },
    functionType=volumeDensity,
    coordinateType=diameter,
    normalise=yes
)

