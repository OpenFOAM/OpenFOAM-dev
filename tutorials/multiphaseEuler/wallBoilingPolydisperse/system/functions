{
    type            surfaceFieldValue;
    libs            ( "libfieldFunctionObjects.so" );
    log             yes;
    writeFields     false;
    select          patch;
    patch           inlet;
    operation       sum;
    weightField     alphaRhoPhi.gas;
    fields          ( h.gas );
}
inletLiquid
{
    type            surfaceFieldValue;
    libs            ( "libfieldFunctionObjects.so" );
