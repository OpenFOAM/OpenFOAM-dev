FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultValues
{
    alpha.air       1;
    alpha.particles 0;
}

zones
{
    bed
    {
        type        box;

        box         (0 0 -0.1) (0.15 0.5 0.1);

        values
        {
            alpha.air       0.45;
            alpha.particles 0.55;
        }
    }
}


