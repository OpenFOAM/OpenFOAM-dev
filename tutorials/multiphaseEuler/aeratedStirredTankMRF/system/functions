#includeFunc fieldAverage
(
    alpha.gas,
    U.gas,
    U.liquid
)

#includeFunc gasHoldup

#includeFunc patchFlowRate
(
    name=inletGasFlow,
    field=alphaPhi.gas,
    patch=inlet,
    writeControl=timeStep,
    writeInterval=10
)

#includeFunc patchFlowRate
(
    name=outletGasFlow,
    field=alphaPhi.gas,
    patch=outlet,
    writeControl=timeStep,
    writeInterval=10
)

