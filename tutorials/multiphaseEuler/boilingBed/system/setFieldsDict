FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultValues
{
    alpha.gas       0;
    alpha.liquid    1;
    alpha.solid     0;
}

zones
{
    bed
    {
        type            lookup;
        zoneType        cell;

        values
        {
            alpha.gas       0;
            alpha.liquid    0.4;
            alpha.solid     0.6;
        }
    }
}

