FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultValues
{
    alpha.air   1;
    alpha.water 0;
    alpha.solid 0;
}

zones
{
    bed
    {
        type        box;
        zoneType    cell;

        box         (0 0.3333 -0.1) (0.15 0.6501 0.1);

        values
        {
            alpha.air   0.4;
            alpha.water 0;
            alpha.solid 0.6;
        }
    }
}

// ************************************************************************* //
