FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      N2.gas;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.492;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

     inlet
    {
        type            uniformFixedValue;
        uniformValue
        {
            type table;
            values
            (
                (0 0.492)
                (2 0.521)
                (3 0.548)
                (4 0.521)
                (5 0.544)
            );
            interpolationScheme step;
        }
    }
    outlet
    {
        type            inletOutlet;
        phi             phi.gas;
        inletValue      uniform 0.492;
        value           uniform 0.492;
    }
    wall
    {
        type            zeroGradient;
    }
}


