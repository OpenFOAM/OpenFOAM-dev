FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega.gas;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 0.01;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    inlet
    {
        type                turbulentMixingLengthFrequencyInlet;
        mixingLength        0.0002;
        k                   k.gas;
        value               uniform 0.01;
    }
    outlet
    {
        type            inletOutlet;
        phi             phi.gas;
        inletValue      uniform 0.0015;
        value           uniform 0.01;
    }
    wall
    {
        type            omegaWallFunction;
        blended         true;
        value           uniform 0.001;
    }
}


