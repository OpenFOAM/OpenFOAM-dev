FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      physicalProperties.liquid;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type            heRhoThermo;
    mixture         pureMixture;
    transport       const;
    thermo          eConst;
    equationOfState rPolynomial;
    specie          specie;
    energy          sensibleInternalEnergy;
}

pressureWorkAlphaLimit 0.01;

mixture
{
    specie
    {
        molWeight       18.0153;
    }

    equationOfState
    {
        C (0.001278 -2.1055e-06 3.9689e-09 4.3772e-13 -2.0225e-16);
    }

    thermodynamics
    {
        hf          -15866000;
        Cv          4184;
    }

    transport
    {
        mu          8.891e-4;
        Pr          6.13;
    }
}

