FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvConstraints;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

limitp
{
    type       limitPressure;

    min        8e4;
    max        1.2e5;
}

limitTgas
{
    type            limitTemperature;
    cellZone        all;
    min             273;
    max             453;
    phase           gas;
}

limitTliquid
{
    type            limitTemperature;
    cellZone        all;
    min             273;
    max             453;
    phase           liquid;
}


