FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      alpha.oil;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    leftWall
    {
        type            alphaContactAngle;
        contactAngleProperties
        {
            air             { theta0 90; }
            water           { theta0 90; }
            mercury         { theta0 90; }
        }
        value           $internalField;
    }

    rightWall
    {
        type            alphaContactAngle;
        contactAngleProperties
        {
            air             { theta0 90; }
            water           { theta0 90; }
            mercury         { theta0 90; }
        }
        value           $internalField;
    }

    lowerWall
    {
        type            alphaContactAngle;
        contactAngleProperties
        {
            air             { theta0 90; }
            water           { theta0 90; }
            mercury         { theta0 90; }
        }
        value           $internalField;
    }

    atmosphere
    {
        type            inletOutlet;
        phi             phi.oil;
        inletValue      uniform 0;
        value           uniform 0;
    }

    defaultFaces
    {
        type            empty;
    }
}


