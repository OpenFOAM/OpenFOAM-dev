    residualAlpha   1e-15;
}

vapour
{
    type            reactingPhaseModel;

    diameterModel   none;

    residualAlpha   1e-5;
}

blending
{
    default
    {
        type            continuous;
        phase           vapour;
    }
}

surfaceTension
{}

populationBalanceCoeffs
{
    aggregates
    {
        continuousPhase vapour;

        coalescenceModels
        (
            DahnekeInterpolation
            {
                sigma           340e-12;
            }
        );

        binaryBreakupModels
        ();

        breakupModels
        ();
    }
}

