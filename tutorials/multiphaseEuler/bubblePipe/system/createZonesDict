FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      createZonesDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

injection
{
    type        box;
    zoneType    cell;
    box         (0 -1 0.0) (0.005 1 0.01);
}

outlet
{
    type        box;
    zoneType    cell;
    box         (0 -1 0.89) (0.025 1 0.9);
}

