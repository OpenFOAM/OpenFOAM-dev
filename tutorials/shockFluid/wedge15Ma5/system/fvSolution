FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    "rho.*"
    {
        solver          diagonal;
    }

    "U.*"
    {
        solver          smoothSolver;
        smoother        GaussSeidel;

        tolerance       1e-9;
        relTol          0;
    }

    "e.*"
    {
        $U;

        tolerance       1e-10;
    }
}

PIMPLE
{}


