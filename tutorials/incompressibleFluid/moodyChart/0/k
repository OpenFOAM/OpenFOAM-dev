FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

kInitial        0.01;

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform $kInitial;

boundaryField
{
    wall
    {
        type            kqRWallFunction;
        value           uniform $kInitial;
    }

    #includeEtc "caseDicts/setConstraintTypes"
}

