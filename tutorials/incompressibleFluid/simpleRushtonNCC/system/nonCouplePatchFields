// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

U
{
    type            movingWallSlipVelocity;
    value           uniform (0 0 0);
}

nut
{
    type            calculated;
    value           uniform 0;
}

"(p|k|omega|epsilon)"
{
    type            zeroGradient;
}

