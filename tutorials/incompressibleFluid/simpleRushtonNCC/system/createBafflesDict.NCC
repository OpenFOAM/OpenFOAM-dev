FoamFile
{
    format      ascii;
    class       dictionary;
    object      createBafflesDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

internalFacesOnly true;

fields true;

baffles
{
    nonCouple
    {
        type        faceZone;
        zoneName    rotatingZone;

        owner
        {
            name        nonCouple1;
            type        patch;

            patchFields
            {
                #include    "nonCouplePatchFields"
            }
        }

        neighbour
        {
            name        nonCouple2;
            type        patch;

            patchFields
            {
                #include    "nonCouplePatchFields"
            }
        }
    }
}

