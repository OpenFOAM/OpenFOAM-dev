FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultValues
{
    s   0;
}

zones
{
    inlet
    {
        type        cylinder;
        zoneType    face;

        point1 (-0.51 0 0);
        point2 (-0.49 0 0);
        radius 0.05;

        values
        {
            s   1;
        }
    }
}

