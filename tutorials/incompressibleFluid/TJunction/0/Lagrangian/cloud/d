FoamFile
{
    format      ascii;
    class       LagrangianScalarField;
    location    "0/Lagrangian/cloud";
    object      d;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [length];

internalField   uniform 0.1 [mm];

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    ".*"
    {
        type            calculated;
    }
}

sources
{
    inlet
    {
        type            distribution;
        distribution
        {
            type            RosinRammler;
            Q               0;
            min             0.02 [mm];
            max             0.2 [mm];
            d               0.1 [mm];
            n               2;
        }
    }
}


