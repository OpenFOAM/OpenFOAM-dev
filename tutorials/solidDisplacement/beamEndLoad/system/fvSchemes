FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

d2dt2Schemes
{
    default         steadyState;
}

gradSchemes
{
    default         Gauss cubic grad(D);
    grad(D)         Gauss linear;
}

divSchemes
{
    default         Gauss cubic grad(D);
}

laplacianSchemes
{
    default         Gauss linear uncorrected;
}

interpolationSchemes
{
    default         cubic grad(D);
}

snGradSchemes
{
    default         uncorrected;
}


