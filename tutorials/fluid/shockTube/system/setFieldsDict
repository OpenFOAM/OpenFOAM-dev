FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultValues
{
    U       (0 0 0);
    T       348.432;
    p       1 [bar];
}

zones
{
    lowPressure
    {
        type        box;

        box         (0 -1 -1) (5 1 1);

        values
        {
            T       278.746;
            p       0.1 [bar];
        }
    }
}


