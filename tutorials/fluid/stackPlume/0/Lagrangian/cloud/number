FoamFile
{
    format      ascii;
    class       LagrangianScalarField;
    location    "0/Lagrangian/cloud";
    object      number;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [];

internalField   uniform 0;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    ".*"
    {
        type        calculated;
    }
}

sources
{
    inlet
    {
        type            flowRateNumber;
        uniformSize     volume;
        massFlowRate    0.004 [kg/s]; // 1kg/250s = 0.004kg/s
    }
}


