FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      fvOptions;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

options
{
    energySource
    {
        type            scalarSemiImplicitSource;

        timeStart       0;
        duration        1;
        selectionMode   cellSet;
        cellSet         ignition;
        volumeMode      specific;

        injectionRateSuSp
        {
            e          (5e7 0); // kg/m/s^3
        }
    }
}


