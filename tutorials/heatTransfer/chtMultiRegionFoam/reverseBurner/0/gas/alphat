FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0/gas";
    object      alphat;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    "inlet.*"
    {
        type            calculated;
        value           $internalField;
    }
    outlet
    {
        type            calculated;
        value           $internalField;
    }
    gas_to_solid
    {
        type            compressible::alphatWallFunction;
        value           $internalField;
    }
}


// ************************************************************************* //
