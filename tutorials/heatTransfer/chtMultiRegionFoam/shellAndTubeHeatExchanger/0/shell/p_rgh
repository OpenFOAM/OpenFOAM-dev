FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/shell";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 1 -1 -2 0 0 0 0 ];

internalField   uniform 0;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    lower
    {
        type            fixedValue;
        value           $internalField;
    }
    upper
    {
        type            fixedFluxPressure;
        value           $internalField;
    }
    wall
    {
        type            fixedFluxPressure;
        value           $internalField;
    }
}

// ************************************************************************* //
