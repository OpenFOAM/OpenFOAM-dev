FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0/shell";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

/*
r=0.008
mDot=0.05
rho=1000
I=0.05
L=r
Cmu=0.09

A=np.pi*r**2
V=mDot/A/rho
k=1.5*V**2*I
epsilon=Cmu**0.75*k**1.5/L
*/

dimensions      [ 0 2 -2 0 0 0 0 ];

internalField   uniform 0.00463812;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    lower
    {
        type            inletOutlet;
        value           $internalField;
        inletValue      $internalField;
    }
    upper
    {
        type            turbulentIntensityKineticEnergyInlet;
        intensity       0.05;
        value           $internalField;
    }
    wall
    {
        type            kqRWallFunction;
        value           $internalField;
    }
}

// ************************************************************************* //
