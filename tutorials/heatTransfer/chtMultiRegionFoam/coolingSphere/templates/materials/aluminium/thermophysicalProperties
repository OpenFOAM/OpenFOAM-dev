FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      thermophysicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type            heSolidThermo;
    mixture         pureMixture;
    transport       constIso;
    thermo          hConst;
    equationOfState rhoConst;
    specie          specie;
    energy          sensibleEnthalpy;
}

mixture
{
    specie
    {
        nMoles      1;
        molWeight   27; // [g/mol]
    }

    transport
    {
        kappa   240;    // [W/(m K)]
    }

    thermodynamics
    {
        Hf      0;
        Cp      896;    // [J/(kg K)]
    }

    equationOfState
    {
        rho     2712;   // [kg/m3]
    }
}

