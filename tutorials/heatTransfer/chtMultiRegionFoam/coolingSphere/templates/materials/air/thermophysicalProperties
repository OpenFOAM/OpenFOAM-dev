FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      thermophysicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type            heRhoThermo;
    mixture         pureMixture;
    transport       const;
    thermo          hConst;
    equationOfState perfectGas;
    specie          specie;
    energy          sensibleEnthalpy;
}

mixture
{
    specie
    {
        nMoles      1;
        molWeight   28.9;       // [g/mol]
    }
    thermodynamics
    {
        Cp          1005;       // [J/(kg K)]
        Hf          0;
    }
    transport
    {
        mu          1.8e-05;    // [kg/(m s)]
        Pr          0.7;
    }
}


