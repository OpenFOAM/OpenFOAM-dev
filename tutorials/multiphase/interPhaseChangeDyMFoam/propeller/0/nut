FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    outerCylinder
    {
        type            nutkWallFunction;
        value           uniform 0;
    }
    propellerTip
    {
        type            nutkWallFunction;
        value           uniform 0;
    }
    propellerStem1
    {
        type            nutkWallFunction;
        value           uniform 0;
    }
    propellerStem2
    {
        type            nutkWallFunction;
        value           uniform 0;
    }
    propellerStem3
    {
        type            nutkWallFunction;
        value           uniform 0;
    }
    AMI1
    {
        type            cyclicAMI;
        value           uniform 0;
    }
    AMI2
    {
        type            cyclicAMI;
        value           uniform 0;
    }
    inlet
    {
        type            calculated;
        value           uniform 0;
    }
    outlet
    {
        type            calculated;
        value           uniform 0;
    }
}


