FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system/plateRegion";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         Euler;
}

gradSchemes
{
    default         Gauss linear;
}

divSchemes
{
    default         none;

    div(phid,alpha) Gauss upwind;
    div(phi,U)      Gauss upwind;
    div(phi,e)      Gauss upwind;
}

laplacianSchemes
{
    default         Gauss linear uncorrected;
}


snGradSchemes
{
    default         uncorrected;
}


