// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

mover
{
    type            motionSolver;

    libs            ("libfvMeshMovers.so" "libfvMotionSolvers.so");

    motionSolver    solidBody;

    solidBodyMotionFunction SDA;

    CofG            (0 0 0);
    lamda           50;
    rollAmax        0.22654;
    rollAmin        0.10472;
    heaveA          3.79;
    swayA           2.34;
    Q               2;
    Tp              13.93;
    Tpn             11.93;
    dTi             0.059;
    dTp             -0.001;
}



