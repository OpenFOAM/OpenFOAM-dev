FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.water 0
);

regions
(
    boxToCell
    {
        box (0 0 0) (50 130 27);
        fieldValues
        (
            volScalarFieldValue alpha.water 1
        );
    }

    boxToFace
    {
        box (0 0 0) (50 10.0001 24);
        fieldValues
        (
            volScalarFieldValue alpha.water 1
        );
    }
);


