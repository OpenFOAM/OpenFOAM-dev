FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application             interPhaseChangeFoam;

startFrom               latestTime;

startTime               0;

stopAt                  endTime;

endTime                 0.05;

deltaT                  1e-8;

writeControl            adjustableRunTime;

writeInterval           0.001;

purgeWrite              0;

writeFormat             ascii;

writePrecision          6;

writeCompression        off;

timeFormat              general;

runTimeModifiable       yes;

adjustTimeStep          on;

maxCo                   5;


