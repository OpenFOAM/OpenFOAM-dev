Dab           [0 2 -1 0 0 0 0]    1e-06;
alphatab      [0 0 0 0 0 0 0]     1;

phases (sludge water);

sludge
{
    transportModel  Newtonian;
    nu              [0 2 -1 0 0 0 0] 1e-06;
    rho             [1 -3 0 0 0 0 0] 1000;
}

water
{
    transportModel  Newtonian;
    nu              [0 2 -1 0 0 0 0] 1e-06;
    rho             [1 -3 0 0 0 0 0] 990;
}

