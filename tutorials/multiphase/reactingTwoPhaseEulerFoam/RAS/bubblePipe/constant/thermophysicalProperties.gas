FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      thermophysicalProperties.gas;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type            heRhoThermo;
    mixture         pureMixture;
    transport       const;
    thermo          hConst;
    equationOfState rhoConst;
    specie          specie;
    energy          sensibleEnthalpy;
}

mixture
{
    specie
    {
        nMoles      1;
        molWeight   18.015;
    }
    equationOfState
    {
        rho         1.17;
    }
    thermodynamics
    {
        Cp          1006.3;
        Hf          0;
    }
    transport
    {
        mu          1.868e-05;
        Pr          0.71;
    }
}
