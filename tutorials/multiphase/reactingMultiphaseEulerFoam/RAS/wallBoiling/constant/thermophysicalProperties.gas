FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      thermophysicalProperties.gas;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type            heRhoThermo;
    mixture         pureMixture;
    transport       const;
    thermo          eConst;
    equationOfState perfectGas;
    specie          specie;
    energy          sensibleInternalEnergy;
}

mixture
{
    specie
    {
        nMoles          1;
        molWeight       195; //Adjusted to match density to reference data
    }
    thermodynamics
    {
        Hf          0;
        Cv          1252;
        Tref        359.981;
        Esref       363410;
    }
    transport
    {
        mu          1.6374e-5;
        Pr          1.19;
    }
}
