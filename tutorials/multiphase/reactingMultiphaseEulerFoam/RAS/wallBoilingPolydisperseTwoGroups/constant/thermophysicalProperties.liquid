FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      thermophysicalProperties.liquid;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type            heRhoThermo;
    mixture         pureMixture;
    transport       const;
    thermo          eRefConst;
    equationOfState rhoConst;
    specie          specie;
    energy          sensibleInternalEnergy;
}

mixture
{
    specie
    {
        nMoles          1;
        molWeight       120.914;
    }
    equationOfState
    {
        rho        1017.1;
    }
    thermodynamics
    {
        Hf          0;
        Cv          1394.5;
        Tref        359.981;
        Eref        290090;
    }
    transport
    {
        mu          8.9e-5;
        Pr          2.8;
    }
}
