FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      physicalProperties.u;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type            ubRhoThermo;
    mixture         uHomogeneousMixture;
    transport       const;
    thermo          hConst;
    equationOfState perfectGas;
    specie          specie;
    energy          sensibleEnthalpy;
}

stoichiometricAirFuelMassRatio 15.675;

Phi     1;

reactants
{
    specie
    {
        molWeight       29.4649;
    }
    thermodynamics
    {
        Cp              1000;
        hf              1700e3;
    }
    transport
    {
        mu              1e-5;
        Pr              1;
    }
}

