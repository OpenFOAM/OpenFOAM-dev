FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant/incompressibleVoF";
    object      fvModels;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

droplets
{
    type        kinematicParticleCloud;
    libs        ("libLagrangianCloud.so");

    phase       water;
    carrierPhase air;
}

particles
{
    type        dynamicParticleCloud;
    libs        ("libLagrangianCloud.so");
}

