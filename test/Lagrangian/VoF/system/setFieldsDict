FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultValues
{
    alpha.water     0;
}

zones
{
    waterColumn
    {
        type            box;

        box             (-1e6 -1e6 -1e6) (1e6 1e6 0.2);

        values
        {
            alpha.water     1;
        }
    }
}


