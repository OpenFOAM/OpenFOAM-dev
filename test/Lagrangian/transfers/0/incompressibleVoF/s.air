FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0/incompressibleVoF";
    object      s.air;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [];

internalField   uniform 0;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    bottom
    {
        type            zeroGradient;
    }

    top
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
}

sources
{
    droplets
    {
        type            uniformFixedValue;
        uniformValue    1;
    }
}


