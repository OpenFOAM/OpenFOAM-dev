FoamFile
{
    format      ascii;
    class       LagrangianScalarField;
    location    "0/Lagrangian/droplets";
    object      d;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [length];

internalField   uniform -1;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    ".*"
    {
        type            calculated;
    }
}

sources
{
    injection
    {
        type            uniformFixedValue;
        uniformValue    2e-4;
    }
}


