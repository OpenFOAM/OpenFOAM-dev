FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant/Lagrangian/droplets";
    object      LagrangianModels;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

injection
{
    type        manualInjection;
    positions   (($x 0 0.499999)) [m];
}

gravity
{
    type        gravity;
}

drag
{
    type        SchillerNaumannDrag;
}

constantFluxCarrierTransfer
{
    type        constantFluxCarrierTransfer;
    volumeFlux  $volumeFlux;
}

