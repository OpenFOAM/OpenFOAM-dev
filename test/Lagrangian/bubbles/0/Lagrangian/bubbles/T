FoamFile
{
    format      ascii;
    class       LagrangianScalarField;
    location    "0/Lagrangian/bubbles";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [temperature];

internalField   uniform 300;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    ".*"
    {
        type            calculated;
    }
}

sources
{
    injection
    {
        type            uniformFixedValue;
        uniformValue    300;
    }
}


