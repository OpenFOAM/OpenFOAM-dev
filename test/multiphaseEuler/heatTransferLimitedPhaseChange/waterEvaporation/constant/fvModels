FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      fvModels;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phaseChange
{
    type            heatTransferLimitedPhaseChange;
    libs            ("libmultiphaseEulerFvModels.so");

    phases          (steam water);

    energySemiImplicit yes;
    pressureImplicit no;

    saturationTemperature
    {
        type            constant;
        value           372.76;
    }
}

