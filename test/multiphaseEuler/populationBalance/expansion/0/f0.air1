FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      f0.air1;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [];

internalField   uniform 1;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    inlet
    {
        type            fixedValue;
        value           $internalField;
    }

    outlet
    {
        type            zeroGradient;
    }
}

sources
{
    massSource
    {
        type        uniformFixedValue;
        uniformValue 1;
    }
}

