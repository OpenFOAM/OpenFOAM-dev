// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

waterSource
{
    type            massSource;

    select          all;

    massFlowRate    30.785654;

    phase           water;
    rho             rho.water;

    fieldValues
    {
        U.water     (0 0 0);
    }
}

