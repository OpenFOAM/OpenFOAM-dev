FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 1e-08;

boundaryField
{
    lowerWall
    {
        type            epsilonWallFunction;
        value           uniform 1e-08;
    }
    upperWall
    {
        type            epsilonWallFunction;
        value           uniform 1e-08;
    }
    front
    {
        type            cyclic;
    }
    back
    {
        type            cyclic;
    }
    defaultFaces
    {
        type            empty;
    }
}


// ************************************************************************* //
