FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      fvModels;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

massSource
{
    type            massSource;

    selectionMode   cellZone;
    cellZone        injection;

    massFlowRate    1e-3;

    phase           air1;
    rho             thermo:rho.air1;

    fieldValues
    {
        f0.air1     1;
        f1.air1     0;
        f2.air1     0;
        f3.air1     0;
        f4.air1     0;
        f5.air1     0;
        U.air1      (0 0 0);
    }
}

