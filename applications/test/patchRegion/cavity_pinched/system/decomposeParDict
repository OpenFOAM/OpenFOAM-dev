FoamFile
{
    format      ascii;
    class       dictionary;
    object      decomposeParDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

numberOfSubdomains  2;

method          hierarchical;

hierarchicalCoeffs
{
    n           (2 1 1);
    order       xyz;
}

