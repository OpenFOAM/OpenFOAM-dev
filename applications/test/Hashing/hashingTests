// edges are hashed commutatively
edgeList
(
    (0 1)
    (1 0)
    (45 100)
    (100 45)
    (128 1000)
    (1000 128)
)

// triFaces are also hashed commutatively (via multiply/add)
triFaceList
(
    (10 20 30)
    (30 20 10)
    (20 10 30)
)


